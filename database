CREATE DATABASE thithuchanhjava2;
   CREATE TABLE student (
    studentID INT NOT NULL,
    name varchar(200),
    address varchar(250),
    phone varchar(25),
    PRIMARY KEY (studentID)
) ENGINE = INNODB
